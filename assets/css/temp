  /* changes header to a column when on mobile */
  header {
    display: flex;
    flex-direction: column;
    justify-content: center;
  
  }

  /* changes nav links to column on mobile */
  header nav {
    display: flex;
    flex-direction: column;
    align-items: center;
  }
}
